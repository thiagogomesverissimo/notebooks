    l2 = "http://mlbsfx.sibi.usp.br:3410/sfxlcl41?url\_ver=Z39.88-2004\&rft\_val\_fmt=info:ofi/fmt:kev:mtx:journal\&genre=article\&sid=ProQ:ProQ\%3Apubliccontent\&atitle=A+Current+Overview+of+the+Use+of+Learning+Analytics+Dashboards\&title=Education+Sciences\&issn=\&date=2024-01-01\&volume=14\&issue=1\&spage=82\&au=Masiello\%2C+Italo\%3BZeynab+\%28Artemis\%29+Mohseni\%3BPalma\%2C+Francis\%3BNordmark\%2C+Susanna\%3BAugustsson\%2C+Hanna\%3BRundquist\%2C+Rebecka\&isbn=\&jtitle=Education+Sciences\&btitle=\&rft\_id=info:eric/\&rft\_id=info:doi/10.3390\%2Feducsci14010082, http://link.periodicos.capes.gov.br/sfxlcl41?url\_ver=Z39.88-2004\&rft\_val\_fmt=info:ofi/fmt:kev:mtx:journal\&genre=article\&sid=ProQ:ProQ\%3Apubliccontent\&atitle=A+Current+Overview+of+the+Use+of+Learning+Analytics+Dashboards\&title=Education+Sciences\&issn=\&date=2024-01-01\&volume=14\&issue=1\&spage=82\&au=Masiello\%2C+Italo\%3BZeynab+\%28Artemis\%29+Mohseni\%3BPalma\%2C+Francis\%3BNordmark\%2C+Susanna\%3BAugustsson\%2C+Hanna\%3BRundquist\%2C+Rebecka\&isbn=\&jtitle=Education+Sciences\&btitle=\&rft\_id=info:eric/\&rft\_id=info:doi/10.3390\%2Feducsci14010082",
