    l2 = "http://mlbsfx.sibi.usp.br:3410/sfxlcl41?url\_ver=Z39.88-2004\&rft\_val\_fmt=info:ofi/fmt:kev:mtx:journal\&genre=article\&sid=ProQ:ProQ\%3Apubliccontent\&atitle=Work+Route+for+the+Inclusion+of+Learning+Analytics+in+the+Development+of+Interactive+Multimedia+Experiences+for+Elementary+Education\&title=Applied+Sciences\&issn=\&date=2024-01-01\&volume=14\&issue=17\&spage=7645\&au=Solano\%2C+Andr\%C3\%A9s\%3BPel\%C3\%A1ez\%2C+Carlos+Alberto\%3BOspina\%2C+Johann+A\%3BLuna-Garc\%C3\%ADa\%2C+Huizilopoztli\%3BJorge+Andrick+Parra\%3BRam\%C3\%ADrez\%2C+Gabriel+Mauricio\%3BMoreira\%2C+Fernando\%3BJes\%C3\%BAs+Alfonso+L\%C3\%B3pez+Sotelo\%3BKlinge+Orlando+Villalba-Condori\&isbn=\&jtitle=Applied+Sciences\&btitle=\&rft\_id=info:eric/\&rft\_id=info:doi/10.3390\%2Fapp14177645, http://link.periodicos.capes.gov.br/sfxlcl41?url\_ver=Z39.88-2004\&rft\_val\_fmt=info:ofi/fmt:kev:mtx:journal\&genre=article\&sid=ProQ:ProQ\%3Apubliccontent\&atitle=Work+Route+for+the+Inclusion+of+Learning+Analytics+in+the+Development+of+Interactive+Multimedia+Experiences+for+Elementary+Education\&title=Applied+Sciences\&issn=\&date=2024-01-01\&volume=14\&issue=17\&spage=7645\&au=Solano\%2C+Andr\%C3\%A9s\%3BPel\%C3\%A1ez\%2C+Carlos+Alberto\%3BOspina\%2C+Johann+A\%3BLuna-Garc\%C3\%ADa\%2C+Huizilopoztli\%3BJorge+Andrick+Parra\%3BRam\%C3\%ADrez\%2C+Gabriel+Mauricio\%3BMoreira\%2C+Fernando\%3BJes\%C3\%BAs+Alfonso+L\%C3\%B3pez+Sotelo\%3BKlinge+Orlando+Villalba-Condori\&isbn=\&jtitle=Applied+Sciences\&btitle=\&rft\_id=info:eric/\&rft\_id=info:doi/10.3390\%2Fapp14177645",
