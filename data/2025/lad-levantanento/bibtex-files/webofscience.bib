Title = {Learning with Explainable AI-Recommendations at School: Extracting
   Patterns of Self-directed Learning from Learning Logs},
Booktitle = {2023 IEEE INTERNATIONAL CONFERENCE ON ADVANCED LEARNING TECHNOLOGIES,
   ICALT},
