@inproceedings{ WOS:000468396900122,
Author = {Azcona, David and Hsiao, I-Han and Smeaton, Alan F.},
Book-Group-Author = {IEEE},
Title = {Personalizing Computer Science Education by Leveraging Multimodal
   Learning Analytics},
Booktitle = {2018 IEEE FRONTIERS IN EDUCATION CONFERENCE (FIE)},
