Title = {An Experimental Study into the Effects of an Advisory Dashboard on
   Students' Online and Offline Learning},
Booktitle = {TECHNOLOGY ENHANCED LEARNING FOR INCLUSIVE AND EQUITABLE QUALITY
   EDUCATION, PT II, EC-TEL 2024},
Series = {Lecture Notes in Computer Science},
