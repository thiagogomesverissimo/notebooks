@inproceedings{ WOS:000871898800030,
Author = {Villalobos, Esteban and Perez-Sanagustin, Mar and Sanza, Cedric and
   Tricot, Andre and Broisin, Julien},
Editor = {Hilliger, I and Munoz-Merino, PJ and DeLaet, T and Ortega-Arranz, A and Farrell, T},
Title = {Supporting Self-regulated Learning in BL: Exploring Learners' Tactics
   and Strategies},
Booktitle = {EDUCATING FOR A NEW FUTURE: MAKING SENSE OF TECHNOLOGY-ENHANCED LEARNING
   ADOPTION, EC-TEL 2022},
