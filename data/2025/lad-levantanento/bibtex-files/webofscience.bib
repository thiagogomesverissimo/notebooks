@inproceedings{ WOS:000775763200015,
Author = {Taibi, Davide and Bianchi, Francesca and Kemkes, Philipp and Marenzi,
   Ivana},
Editor = {McLaren, B and Reilly, R and Zvacek, S and Uhomoibhi, J},
Title = {Learning Analytics for Interpreting},
Booktitle = {CSEDU: PROCEEDINGS OF THE 10TH INTERNATIONAL CONFERENCE ON COMPUTER
   SUPPORTED EDUCATION - VOL 1},
Year = {2019},
Pages = {145-154},
Note = {10th International Conference on Computer Supported Education (CSEDU),
   Funchal, PORTUGAL, MAR 15-17, 2018},
Abstract = {An important activity in the life of interpreters is terminology work. A
   primary method for learning technical vocabulary is the creation of
   personal glossaries. The current paper describes the design and creation
   of a system that guides the students in autonomous vocabulary work,
   supports the students' learning progress, and helps the teacher in
   monitoring the student commitment to and achievements in the creation of
   personal glossaries. The system includes a tool for the creation of
