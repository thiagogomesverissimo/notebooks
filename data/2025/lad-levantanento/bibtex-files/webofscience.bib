@inproceedings{ WOS:000883327600043,
Author = {Ng, Jeremy Tzi-Dong and Wang, Zuo and Hu, Xiao},
Book-Group-Author = {Assoc Comp Machinery},
Title = {Needs Analysis and Prototype Evaluation of Student-facing LA Dashboard
   for Virtual Reality Content Creation},
Booktitle = {LAK22 CONFERENCE PROCEEDINGS: THE TWELFTH INTERNATIONAL CONFERENCE ON
   LEARNING ANALYTICS \& KNOWLEDGE},
Year = {2022},
