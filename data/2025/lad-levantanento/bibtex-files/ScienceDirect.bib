author = {Philip Doty},
keywords = {Library analytics, Surveillance, Big data, Moral dilemmas, Academic libraries},
abstract = {Library analytics in academic libraries, especially in the United States, are part of the expanding web of society-wide surveillance. The use of library analytics systems poses serious threats to the well-being of many actors, especially students. Academic librarians are faced with new moral challenges by such systems as described here and must determine the extent to which they wish to participate in the surveillance of students for whom they are responsible. The column concludes with some recommendations from the literature and with consideration of the roles of free inquiry, freedom from surveillance, and exercise of autonomy essential to higher education and full citizenship.}
}
@article{MEHTA2025758,
